---
import { Image } from "astro:assets";
import logo from "@images/logo.svg";

const { width, height, ...rest } = Astro.props;

interface Props {
  width?: number;
  height?: number;
  class?: string;
}
---

<Image
  src={logo}
  alt="Invoice Caddy Logo"
  width={width}
  height={height}
  {...rest}
/>
